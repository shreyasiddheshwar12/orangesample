  "instructions_to_main_agent": {
    "setup": [
      "Install 'lucide-react' for icons.",
      "Install 'framer-motion' for animations.",
      "Update 'tailwind.config.js' with the colors and border radius provided in 'design_system.colors.tailwind_config_override'.",
      "Add the Google Fonts import to 'index.css'."
    ],
    "implementation_rules": [
      "Use 'Syne' font for all Headings (h1-h3).",
      "Use 'Plus Jakarta Sans' for all Body text.",
      "Buttons must be 'rounded-full' (pill shape).",
      "Cards must be 'rounded-3xl' (super rounded).",
      "Use the 'microcopy_examples' for placeholders, success messages, and empty states.",
      "For the 'Creator Marketplace', use a masonry or bento grid layout, not a simple table.",
      "Ensure the 'Primary' orange color is used for main CTAs.",
      "Use 'Secondary' peach color for backgrounds of active states or accents.",
      "Add 'data-testid' to all interactive elements."
    ],
    "universal_guidelines": [
      "Do not use gradients for dark colors.",
      "Avoid generic centered layouts; use asymmetry where possible.",
      "Use 2-3x more spacing than typical designs (p-8, p-12).",
      "Every interaction needs a micro-animation (hover scale, tap shrink).",
      "Use 'sonner' for toasts."
    ]
  }
